<?xml version="1.0" encoding="UTF-8"?>
<web-app
   xmlns="http://java.sun.com/xml/ns/javaee"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation="
      http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
   
   <env-entry>
      <env-entry-name>name</env-entry-name>
      <env-entry-type>java.lang.String</env-entry-type>
      <env-entry-value>Resin</env-entry-value>
   </env-entry>

   <!-- Should be overridden by jetty-env.xml -->
   <env-entry>
      <env-entry-name>type</env-entry-name>
      <env-entry-type>java.lang.String</env-entry-type>
      <env-entry-value>Embedded</env-entry-value>
   </env-entry>

</web-app>
